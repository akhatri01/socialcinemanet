<h1>Listing users</h1>

<table>
  <tr>
    <th>Fname</th>
    <th>Mname</th>
    <th>Lname</th>
    <th>Dob</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.fname %></td>
    <td><%= user.mname %></td>
    <td><%= user.lname %></td>
    <td><%= user.dob %></td>
  </tr>
<% end %>
</table>

